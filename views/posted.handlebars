<header>
    <h1>
        <a href="/signedin">Home</a>
        <a href="/posted">Dashboard</a>
        <button id="logout" class="btn-no-style">Logout</button>
    </h1>
</header>
</br>
<ol class="post-list">
    {{#each posts as |post|}}
    <li>
        <article class="post">
        <div class="title">
            <a href="{{post.post_url}}" target="_blank">{{post.title}}</a>
            <span>({{post.post_url}})</span>
        </div>
        <div class="meta">
            by {{post.user.username}} on {{post.created_at}} with detailed description: {{post.text}}
            click here to update or delete post 

            <a href="/update">editing post:</a>
            <a href="/post/{{id}}">{{post.comments.length}} comment(s)</a>
            </div>
        </article>
    </li>
    {{/each}}
</ol>
<a href="/blog",>Create Your Own Post</a>
<script src="javascript/logout.js"></script>
